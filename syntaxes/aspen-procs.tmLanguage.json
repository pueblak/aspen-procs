{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ASPEN PROCS",
	"fileTypes": [
		".EXT"
	],
	"patterns": [
		{ "include": "#files" },
		{ "include": "#comments" },
		{ "include": "#labels" },
		{ "include": "#variables" },
		{ "include": "#commands" },
		{ "include": "#storages" },
		{ "include": "#synonyms" },
		{ "include": "#constants" },
		{ "include": "#strings" },
		{ "include": "#escape" }
	],
	"repository": {
		"constants": {
			"patterns": [{
				"name": "constant.character.numeric.aspen-procs",
				"match": "(?<=[,(\\s])-?\\b\\d+(\\.\\d+)?\\b"
			}]
		},
		"variables": {
			"patterns": [{
				"name": "support.other.synoynm.aspen-procs",
				"match": "^((S\\s+AS)|(S\\s+DS)|(C\\s+IC)|(C\\s+DC))\\s+([a-zA-Z_](\\w|-)*)(.+)?",
				"captures": {
					"1": {
						"name": "keyword.control.reference.aspen-procs"
					},
					"2": {
						"name": "support.function.declare.aspen-procs"
					},
					"6": {
						"patterns": [{
							"name": "keyword.control.reserved.aspen-procs",
							"match": "\\b(LDA\\$\\$\\$|ASPEN_HOME|AUTHNO|COBOL(PGM|_SUFFIX|_TAIL)?|COL|DATE|DEOP|EOPMESS|LINK(DATE|TIME)|MENUHEAD|(MAIN|NEXT)MENU|MODE|OS|PATH|PROCDUMP|PROMPT_VAL|RMRC|ROW|RUNPATH|ST_DIR|START|SYSRC|TERMNO|TIME|USER(ID|TERM)|ARGS_\\d+|RETURN)\\b"
						},
						{
							"name": "support.function.declare.aspen-procs",
							"match": ".+"
						}]
					},
					"8": {
						"name": "support.other.text.aspen-procs",
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#strings"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "support.other.text.aspen-procs",
				"match": "^(\\/\\s+RD)\\s+(\\(\\d+\\s*,\\s*\\d+\\)\\s+)?([a-zA-Z_](\\w|-)*)(\\s+\\S+)?",
				"captures": {
					"1": {
						"name": "keyword.control.read.aspen-procs"
					},
					"2": {
						"name": "constant.character.numeric.aspen-procs"
					},
					"3": {
						"patterns": [{
							"name": "keyword.control.reserved.aspen-procs",
							"match": "\\b(LDA\\$\\$\\$|ASPEN_HOME|AUTHNO|COBOL(PGM|_SUFFIX|_TAIL)?|COL|DATE|DEOP|EOPMESS|LINK(DATE|TIME)|MENUHEAD|(MAIN|NEXT)MENU|MODE|OS|PATH|PROCDUMP|PROMPT_VAL|RMRC|ROW|RUNPATH|ST_DIR|START|SYSRC|TERMNO|TIME|USER(ID|TERM)|ARGS_\\d+|RETURN)\\b"
						},
						{
							"name": "support.function.declare.aspen-procs",
							"match": ".+"
						}]
					},
					"5": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#strings"
						},
						{
							"include": "#escape"
						}]
					}
				}
			}]
		},
		"synonyms": {
			"patterns": [{
				"name": "keyword.control.reserved.aspen-procs",
				"match": "@(LDA\\$\\$\\$|ASPEN_HOME|AUTHNO|COBOL(PGM|_SUFFIX|_TAIL)?|COL|DATE|DEOP|EOPMESS|LINK(DATE|TIME)|MENUHEAD|(MAIN|NEXT)MENU|MODE|OS|PATH|PROCDUMP|PROMPT_VAL|RMRC|ROW|RUNPATH|ST_DIR|START|SYSRC|TERMNO|TIME|USER(ID|TERM)|ARGS_\\d+|RETURN)\\b(\\.|(\\(\\s*((@|\\w|-|\\.)+)\\s*,\\s*((@|\\w|-|\\.)+)\\s*\\)))?",
				"captures": {
					"7": {
						"name": "constant.character.access.aspen-procs"
					},
					"8": {
						"patterns": [{
							"name": "keyword.control.operator.aspen-procs",
							"match": "L|R"
						},
						{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"10": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "constant.character.special.aspen-procs",
				"match": "@(\\$(\\d+|(@+[a-zA-Z_](\\w|-)*\\.*)))(\\.+|(\\(\\s*((@|\\w|-|\\.)+)\\s*,\\s*((@|\\w|-|\\.)+)\\s*\\)))?",
				"captures": {
					"1": {
						"patterns": [{
							"name": "keyword.control.special.aspen-procs",
							"match": "\\$\\d+"
						}]
					},
					"6": {
						"name": "constant.character.access.aspen-procs"
					},
					"7": {
						"patterns": [{
							"name": "keyword.control.operator.aspen-procs",
							"match": "L|R"
						},
						{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"9": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "constant.character.reference.aspen-procs",
				"match": "@+[a-zA-Z_](\\w|-)*(\\.+|(\\(\\s*((@|\\w|-|\\.)+)\\s*,\\s*((@|\\w|-|\\.)+)\\s*\\)))?",
				"captures": {
					"4": {
						"name": "constant.character.access.aspen-procs"
					},
					"5": {
						"patterns": [{
							"name": "keyword.control.operator.aspen-procs",
							"match": "L|R"
						},
						{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"7": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					}
				}
			}]
		},
		"labels": {
			"patterns": [{
				"name": "variable.parameter.label.aspen-procs",
				"match": "^:.{1,10}\\s*$"
			},
			{
				"name": "invalid.deprecated.too-long.aspen-procs",
				"match": "^:.{11,}\\s*$"
			},
			{
				"name": "support.class.success.aspen-procs",
				"match": "(\\((\\w|-|\\.)+\\))|(\\((\\w|-|\\.)*(?=@))|((?<=\\.)(\\w|-|\\.)*\\))"
			},
			{
				"name": "invalid.illegal.failure.aspen-procs",
				"match": "(\\[(\\w|-|\\.|,)+\\])|(\\[(\\w|-|\\.|,)*(?=@))|((?<=\\.)(\\w|-|\\.|,)*\\])"
			},
			{
				"name": "variable.parameter.transfer.aspen-procs",
				"match": "(\\{(\\w|-|\\.)+\\})|(\\{(\\w|-|\\.)*(?=@))|((?<=\\.)(\\w|-|\\.)*\\})"
			},
			{
				"name": "variable.parameter.plaintext.aspen-procs",
				"match": "((?<=\\.)((\\w|-|\\.)+)(?=@))"
			}]
		},
		"storages": {
			"patterns": [{
				"name": "storage.type.aspen-procs",
				"match": "@+\\b(\\d+)(:\\d+)?[^G]\\b"
			},
			{
				"name": "storage.modifier.aspen-procs",
				"match": "@+\\b(\\d+|[a-zA-Z_]\\w*\\.?)(:\\d+)?G\\b"
			}]
		},
		"commands": {
			"patterns": [{
				"name": "constant.character.access.aspen-procs",
				"match": "(?<!\\w)(((\\$|\\d*:)\\d+\\b)|(\\((L|R|\\d+|(@(\\w|-)+\\.?))\\s*,\\s*(\\d+|@(\\w|-)+\\.?)\\)))|(?<!^)\\*",
				"captures": {
					"5": {
						"patterns": [{
							"name": "keyword.control.access.aspen-procs",
							"match": "L|R"
						},
						{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"8": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "support.other.file.aspen-procs",
				"match": "^(\\$)((\\$\\s*$)|(\\s+(\\S+)\\s+(A|N)))",
				"captures": {
					"1": {
						"name": "keyword.control.file.aspen-procs"
					},
					"3": {
						"name": "keyword.control.file.aspen-procs"
					},
					"5": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#strings"
						},
						{
							"include": "#escape"
						}]
					},
					"6": {
						"name": "keyword.control.file.aspen-procs"
					}
				}
			},
			{
				"name": "support.other.system.aspen-procs",
				"match": "^(#)\\s*(.+)$",
				"captures": {
					"1": {
						"name": "keyword.control.command.aspen-procs"
					},
					"2": {
						"name": "support.other.text.aspen-procs",
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#strings"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "support.other.text.aspen-procs",
				"match": "^(\\/\\s+DI)\\s+(\\((\\S+)\\s*,\\s*(\\S+)\\)\\s+)?(.+)$",
				"captures": {
					"1": {
						"name": "keyword.control.command.aspen-procs"
					},
					"2": {
						"name": "constant.character.access.aspen-procs"
					},
					"3": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"4": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#escape"
						}]
					},
					"5": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#strings"
						},
						{
							"include": "commands"
						},
						{
							"include": "#escape"
						}]
					}
				}
			},
			{
				"name": "support.other.text.aspen-procs",
				"match": "^(&\\s+\\$?)(\\S+)\\s+(AUTO|NO_HALT)?",
				"captures": {
					"1": {
						"name": "keyword.control.command.aspen-procs"
					},
					"2": {
						"patterns": [{
							"include": "#storages"
						},
						{
							"include": "#synonyms"
						},
						{
							"include": "#strings"
						},
						{
							"include": "#escape"
						}]
					},
					"3": {
						"name": "keyword.control.command.aspen-procs"
					}
				}
			},
			{
				"name": "keyword.control.command.aspen-procs",
				"match": "(^(S (AS|DS)|C (IC|DC)|F (CR|DL|EX)|\\/ (DI|CC|CS|CL|DL|PS)|G AG|L AL|!|#|\\?|&( \\$)?))|(\\b(EQ|GT|LT|NE|GE|LE)\\b)|((\\/ RD(\\s+\\(\\d+\\s*,\\s*\\d+\\))?)(?=\\s))"
			}]
		},
		"files": {
			"name": "markup.italic.file.aspen-procs",
			"begin": "^(?=\\$\\s)",
			"end": "^(?=\\$\\$)",
			"patterns": [
				{ "include": "#comments" },
				{ "include": "#labels" },
				{ "include": "#variables" },
				{ "include": "#commands" },
				{ "include": "#storages" },
				{ "include": "#synonyms" },
				{ "include": "#constants" },
				{ "include": "#strings" },
				{ "include": "#escape" }
			]
		},
		"strings": {
			"name": "string.quoted.double.aspen-procs",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"include": "#escape"
				}
			]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.aspen-procs",
				"match": "^\\*.*"
			}]
		},
		"escape": {
			"patterns": [{
				"name": "entity.name.function.macro.aspen-procs",
				"match": "\\^."
			}]
		}
	},
	"scopeName": "source.aspen-procs"
}